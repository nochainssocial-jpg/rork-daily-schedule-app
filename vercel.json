{
  "buildCommand": "npm install && npx expo export -p web",
  "outputDirectory": "dist",
  "framework": null,
  "installCommand": "npm install",
  "functions": {
    "backend/hono.ts": {
      "runtime": "nodejs20.x"
    }
  },
  "env": {
    "NODE_ENV": "production"
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/backend/hono.ts"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}